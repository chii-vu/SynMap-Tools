{
    "package": "CoGe",
    "name": "GeneOrder",
    "internal": "gene_order",
    "org": "srlab",
    "group": "SynMap",
    "desc": "Reorders the genes contained in the dag file by ascending order",
    "href": "",
    "params": [
        {
            "name": "input",
            "type": "file",
            "desc": "dag file to be ordered",
            "default": ""
        },
        {
            "name": "gid1",
            "type": "str",
            "desc": "first genome id",
            "default": "''"
        },
        {
            "name": "gid2",
            "type": "str",
            "desc": "second genome id",
            "default": "''"
        },
        {
            "name": "feature1",
            "type": "str",
            "desc": "feature of first genome",
            "default": "'CDS'"
        },
        {
            "name": "feature2",
            "type": "str",
            "desc": "feature of second genome",
            "default": "'CDS'"
        }
    ],
    "returns": [
        {
            "name": "output",
            "type": "file"
        }
    ]
}